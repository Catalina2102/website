<h1>All Council Meetings</h1>

{% set years = collections.tc_meetings | getYears %}
{% for year in years %}
  <h2 class="h4">{{ year }}</h2>
  <ul>
    {% for page in collections.tc_meetings | pagesByYear(year) %}
      <li><a href="{{ page.url }}">{{ page.data.title }}</a></li>
    {% endfor %}
  </ul>
{% endfor %}
